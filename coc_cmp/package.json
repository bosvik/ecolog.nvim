{
  "name": "coc-ecolog",
  "version": "0.0.2",
  "description": "Environment variable completion for coc.nvim, powered by ecolog.nvim",
  "author": {
    "name": "philosofonusus",
    "url": "https://github.com/philosofonusus"
  },
  "publisher": "philosofonusus",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "coc.nvim",
    "environment",
    "completion",
    "neovim",
    "env",
    "dotenv",
    "environment-variables"
  ],
  "engines": {
    "coc": "^0.0.82",
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philosofonusus/ecolog.nvim"
  },
  "bugs": {
    "url": "https://github.com/philosofonusus/ecolog.nvim/issues"
  },
  "homepage": "https://github.com/philosofonusus/ecolog.nvim/tree/main/coc",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "coc-ecolog configuration",
      "properties": {
        "ecolog.enable": {
          "type": "boolean",
          "default": true,
          "description": "Enable coc-ecolog"
        }
      }
    }
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && tsc -p tsconfig.json",
    "watch": "tsc -w -p tsconfig.json",
    "prepare": "npm run clean && npm run build",
    "lint": "eslint src --ext ts",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29",
    "@types/node": "^22",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "coc.nvim": "^0.0.82",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^4.9.5"
  },
  "files": [
    "lib",
    "package.json",
    "README.md",
    "LICENSE"
  ]
} 